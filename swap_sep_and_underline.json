{
    "title": "Swap separate and underline",
    "rules": [{
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "hyphen"
            },
            "to": [{
                "key_code": "hyphen",
                "modifiers": ["shift"]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "hyphen",
                "modifiers": {
                    "mandatory": ["shift"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "hyphen"
            }]
        }]
    }]
}
