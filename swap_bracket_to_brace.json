{
    "title": "Swap [ ] to { }",
    "rules": [{
        "description": "Swap [ ] to { }",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "open_bracket"
            },
            "to": [{
                "key_code": "open_bracket",
                "modifiers": ["shift"]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "open_bracket",
                "modifiers": {
                    "mandatory": ["shift"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "open_bracket"
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "close_bracket"
            },
            "to": [{
                "key_code": "close_bracket",
                "modifiers": ["shift"]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "close_bracket",
                "modifiers": {
                    "mandatory": ["shift"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "close_bracket"
            }]
        }]
    }]
}