{
    "title": "CMD+M to Ctrl+M",
    "rules": [{
        "description": "Set left_command+m to left_control+m",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "m",
                "modifiers": {
                    "mandatory": ["left_control"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "m",
                "modifiers": ["left_command"]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "m",
                "modifiers": {
                    "mandatory": ["left_command"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "m",
                "modifiers": ["left_control"]
            }]
        }]
    }]
}