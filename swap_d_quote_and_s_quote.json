{
    "title": "Swap double quote and single quote",
    "rules": [{
        "description":"Swap double quote and single quote",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "quote"
            },
            "to": [{
                "key_code": "quote",
                "modifiers": ["shift"]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "quote",
                "modifiers": {
                    "mandatory": ["shift"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "quote"
            }]
        }]
    }]
}

