{
    "title": "Right CMD+C aim Left Ctrl+C, Right CMD+R aim Left Ctrl+R",
    "rules": [{
        "description": "Set right_command+c aim left_control+c, right_command+r aim left_control+r",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "c",
                "modifiers": {
                    "mandatory": ["right_command"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "c",
                "modifiers": ["left_control"]
            }]
        }, {
            "type": "basic",
            "from": {
                "key_code": "r",
                "modifiers": {
                    "mandatory": ["right_command"],
                    "optional": ["any"]
                }
            },
            "to": [{
                "key_code": "r",
                "modifiers": ["left_control"]
            }]
        }]
    }]
}